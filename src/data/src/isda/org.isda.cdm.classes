func ListsCompare:
	inputs:
		compareOp CompareOp(1..1)
		left number (1..*)
		right number(0..*)
		rightNumber number(0..1)
	output: result boolean (1..1)
	
func Max:
	inputs:
		a number (1..1)
		b number (1..1)
	output:
		max number (1..1)
	
	assign-output max:
		if a > b 
		then a 
		else b
		
